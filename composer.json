{
    "name": "jbrooksuk/laravel-colorize",
    "description": "A handy set of Stringable mixins for CLI text.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com",
            "homepage": "https://james.brooks.page",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0",
        "symfony/console": "^5.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "JamesBrooks\\Colorize\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JamesBrooks\\Colorize\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JamesBrooks\\Colorize\\ColorizeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
     "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/jbrooksuk"
        }
    ]
}
